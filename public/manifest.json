{
  "manifest_version": 2,
  "name": "Transcribr",
  "description": "Transcribr youtube extension",
  "version": "0.0.1",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
 
 "content_security_policy": "script-src 'self' 'sha256-3O7hfOqzYo46dDx9x5bJ0mZWf9ltnA8nLeedT+UDpN4='; object-src 'self'",
"permissions": [
  "storage",
  "tabs"
  ]
}